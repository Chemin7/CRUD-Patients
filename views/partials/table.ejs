<div class="col-sm">
    <% if(patients) {%>
      <table class="table table-bordered">
        <thead>
          <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone</th>
            <th>Birthday</th>
          </tr>
        </thead>
        <tbody>
          <% patients.forEach((patient)=> { %>
            <tr>
              <td>
                <%= patient.id %>
              </td>
              <td>
                <%= patient.first_name %>
              </td>
              <td>
                <%= patient.last_name %>
              </td>
              <td>
                <%= patient.phone %>
              </td>
              <td>
                <%= patient.birthday %>
              </td>

            </tr>
            <% }) %>
        </tbody>
      </table>
      <%}%>
  </div>